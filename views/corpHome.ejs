        <style>
            .custom-card {
                height: 100%; 
            }
        </style>
        
        <% for (let i = 0; i < dataContents.length; i += 6) { %>
            <div class="row">
                <% for (let j = i; j < i + 6 && j < dataContents.length; j++) { %>
                    <div class="col-md-4 mb-4">
                        <div class="card custom-card">
                            <img src="<%= imageUrls[j] %>" class="card-img-top" alt="<%= dataContents[j].title %>">
                            <div class="card-body">
                                <h3 class="card-title"><%= dataContents[j].username %></h3>
                                <h5 class="card-title"><%= dataContents[j].skilldata_id %> Skill Id</h5>
                                <a href="#" class="btn btn-primary">Apply</a>
                            </div>
                        </div>
                    </div>
                <% } %>
            </div>
        <% } %>

        <nav aria-label="Page navigation">
        </nav>
    </div>

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.3/dist/umd/popper.min.js"></script>
